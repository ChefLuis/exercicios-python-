# ==============================
# 3. Calculadora de Volume
# ==============================
def calcular_volume():
    comprimento = 12  # cm
    largura = 14      # cm
    altura = 20       # cm

    volume = comprimento * largura * altura

    print("Calculadora de Volume")
    print(f"Comprimento: {comprimento} cm")
    print(f"Largura: {largura} cm")
    print(f"Altura: {altura} cm")
    print(f"Volume: {volume} cmÂ³")
